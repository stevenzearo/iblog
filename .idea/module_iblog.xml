<?xml version="1.0" encoding="UTF-8"?>
<project name="module_iblog" default="compile.module.iblog">
  <dirname property="module.iblog.basedir" file="${ant.file.module_iblog}"/>
  
  <property name="module.jdk.home.iblog" value="${project.jdk.home}"/>
  <property name="module.jdk.bin.iblog" value="${project.jdk.bin}"/>
  <property name="module.jdk.classpath.iblog" value="${project.jdk.classpath}"/>
  
  <property name="compiler.args.iblog" value="-encoding UTF-8 -source 11 -target 11 ${compiler.args}"/>
  
  <property name="iblog.output.dir" value="undefined"/>
  <property name="iblog.testoutput.dir" value="undefined"/>
  
  <path id="iblog.module.bootclasspath">
    <!-- Paths to be included in compilation bootclasspath -->
  </path>
  
  <path id="iblog.module.production.classpath">
    <path refid="${module.jdk.classpath.iblog}"/>
  </path>
  
  <path id="iblog.runtime.production.module.classpath"/>
  
  <path id="iblog.module.classpath">
    <path refid="${module.jdk.classpath.iblog}"/>
  </path>
  
  <path id="iblog.runtime.module.classpath"/>
  
  
  <patternset id="excluded.from.module.iblog">
    <patternset refid="ignored.files"/>
  </patternset>
  
  <patternset id="excluded.from.compilation.iblog">
    <patternset refid="excluded.from.module.iblog"/>
  </patternset>
  
  
  <target name="compile.module.iblog" depends="compile.module.iblog.production,compile.module.iblog.tests" description="Compile module iblog"/>
  
  <target name="compile.module.iblog.production" depends="register.custom.compilers" description="Compile module iblog; production classes"/>
  
  <target name="compile.module.iblog.tests" depends="register.custom.compilers,compile.module.iblog.production" description="compile module iblog; test classes" unless="skip.tests"/>
  
  <target name="clean.module.iblog" description="cleanup module">
    <delete dir="${iblog.output.dir}"/>
    <delete dir="${iblog.testoutput.dir}"/>
  </target>
</project>