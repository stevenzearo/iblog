<?xml version="1.0" encoding="UTF-8"?>
<project name="module_iblog.main" default="compile.module.iblog.main">
  <dirname property="module.iblog.main.basedir" file="${ant.file.module_iblog.main}"/>
  
  <property name="module.jdk.home.iblog.main" value="${project.jdk.home}"/>
  <property name="module.jdk.bin.iblog.main" value="${project.jdk.bin}"/>
  <property name="module.jdk.classpath.iblog.main" value="${project.jdk.classpath}"/>
  
  <property name="compiler.args.iblog.main" value="-encoding UTF-8 -source 11 -target 11 ${compiler.args}"/>
  
  <property name="iblog.main.output.dir" value="undefined"/>
  <property name="iblog.main.testoutput.dir" value="undefined"/>
  
  <path id="iblog.main.module.bootclasspath">
    <!-- Paths to be included in compilation bootclasspath -->
  </path>
  
  <path id="iblog.main.module.production.classpath">
    <path refid="${module.jdk.classpath.iblog.main}"/>
  </path>
  
  <path id="iblog.main.runtime.production.module.classpath"/>
  
  <path id="iblog.main.module.classpath">
    <path refid="${module.jdk.classpath.iblog.main}"/>
  </path>
  
  <path id="iblog.main.runtime.module.classpath"/>
  
  
  <patternset id="excluded.from.module.iblog.main">
    <patternset refid="ignored.files"/>
  </patternset>
  
  <patternset id="excluded.from.compilation.iblog.main">
    <patternset refid="excluded.from.module.iblog.main"/>
  </patternset>
  
  
  <target name="compile.module.iblog.main" depends="compile.module.iblog.main.production,compile.module.iblog.main.tests" description="Compile module iblog.main"/>
  
  <target name="compile.module.iblog.main.production" depends="register.custom.compilers" description="Compile module iblog.main; production classes"/>
  
  <target name="compile.module.iblog.main.tests" depends="register.custom.compilers,compile.module.iblog.main.production" description="compile module iblog.main; test classes" unless="skip.tests"/>
  
  <target name="clean.module.iblog.main" description="cleanup module">
    <delete dir="${iblog.main.output.dir}"/>
    <delete dir="${iblog.main.testoutput.dir}"/>
  </target>
</project>