<?xml version="1.0" encoding="UTF-8"?>
<project name="module_iblog.backend" default="compile.module.iblog.backend">
  <dirname property="module.iblog.backend.basedir" file="${ant.file.module_iblog.backend}"/>
  
  <property name="module.jdk.home.iblog.backend" value="${project.jdk.home}"/>
  <property name="module.jdk.bin.iblog.backend" value="${project.jdk.bin}"/>
  <property name="module.jdk.classpath.iblog.backend" value="${project.jdk.classpath}"/>
  
  <property name="compiler.args.iblog.backend" value="-encoding UTF-8 -source 11 -target 11 ${compiler.args}"/>
  
  <property name="iblog.backend.output.dir" value="undefined"/>
  <property name="iblog.backend.testoutput.dir" value="undefined"/>
  
  <path id="iblog.backend.module.bootclasspath">
    <!-- Paths to be included in compilation bootclasspath -->
  </path>
  
  <path id="iblog.backend.module.production.classpath">
    <path refid="${module.jdk.classpath.iblog.backend}"/>
  </path>
  
  <path id="iblog.backend.runtime.production.module.classpath"/>
  
  <path id="iblog.backend.module.classpath">
    <path refid="${module.jdk.classpath.iblog.backend}"/>
  </path>
  
  <path id="iblog.backend.runtime.module.classpath"/>
  
  
  <patternset id="excluded.from.module.iblog.backend">
    <patternset refid="ignored.files"/>
  </patternset>
  
  <patternset id="excluded.from.compilation.iblog.backend">
    <patternset refid="excluded.from.module.iblog.backend"/>
  </patternset>
  
  
  <target name="compile.module.iblog.backend" depends="compile.module.iblog.backend.production,compile.module.iblog.backend.tests" description="Compile module iblog.backend"/>
  
  <target name="compile.module.iblog.backend.production" depends="register.custom.compilers" description="Compile module iblog.backend; production classes"/>
  
  <target name="compile.module.iblog.backend.tests" depends="register.custom.compilers,compile.module.iblog.backend.production" description="compile module iblog.backend; test classes" unless="skip.tests"/>
  
  <target name="clean.module.iblog.backend" description="cleanup module">
    <delete dir="${iblog.backend.output.dir}"/>
    <delete dir="${iblog.backend.testoutput.dir}"/>
  </target>
</project>