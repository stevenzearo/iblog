<?xml version="1.0" encoding="UTF-8"?>
<project name="module_iblog.backend.blog-service" default="compile.module.iblog.backend.blog-service">
  <dirname property="module.iblog.backend.blog-service.basedir" file="${ant.file.module_iblog.backend.blog-service}"/>
  
  <property name="module.jdk.home.iblog.backend.blog-service" value="${project.jdk.home}"/>
  <property name="module.jdk.bin.iblog.backend.blog-service" value="${project.jdk.bin}"/>
  <property name="module.jdk.classpath.iblog.backend.blog-service" value="${project.jdk.classpath}"/>
  
  <property name="compiler.args.iblog.backend.blog-service" value="-encoding UTF-8 -source 11 -target 11 ${compiler.args}"/>
  
  <property name="iblog.backend.blog-service.output.dir" value="undefined"/>
  <property name="iblog.backend.blog-service.testoutput.dir" value="undefined"/>
  
  <path id="iblog.backend.blog-service.module.bootclasspath">
    <!-- Paths to be included in compilation bootclasspath -->
  </path>
  
  <path id="iblog.backend.blog-service.module.production.classpath">
    <path refid="${module.jdk.classpath.iblog.backend.blog-service}"/>
  </path>
  
  <path id="iblog.backend.blog-service.runtime.production.module.classpath"/>
  
  <path id="iblog.backend.blog-service.module.classpath">
    <path refid="${module.jdk.classpath.iblog.backend.blog-service}"/>
  </path>
  
  <path id="iblog.backend.blog-service.runtime.module.classpath"/>
  
  
  <patternset id="excluded.from.module.iblog.backend.blog-service">
    <patternset refid="ignored.files"/>
  </patternset>
  
  <patternset id="excluded.from.compilation.iblog.backend.blog-service">
    <patternset refid="excluded.from.module.iblog.backend.blog-service"/>
  </patternset>
  
  
  <target name="compile.module.iblog.backend.blog-service" depends="compile.module.iblog.backend.blog-service.production,compile.module.iblog.backend.blog-service.tests" description="Compile module iblog.backend.blog-service"/>
  
  <target name="compile.module.iblog.backend.blog-service.production" depends="register.custom.compilers" description="Compile module iblog.backend.blog-service; production classes"/>
  
  <target name="compile.module.iblog.backend.blog-service.tests" depends="register.custom.compilers,compile.module.iblog.backend.blog-service.production" description="compile module iblog.backend.blog-service; test classes" unless="skip.tests"/>
  
  <target name="clean.module.iblog.backend.blog-service" description="cleanup module">
    <delete dir="${iblog.backend.blog-service.output.dir}"/>
    <delete dir="${iblog.backend.blog-service.testoutput.dir}"/>
  </target>
</project>